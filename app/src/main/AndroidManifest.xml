<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lenovo.notebook"
    android:versionCode="1"
    android:versionName="0.0.2"
    >

    <application
        android:name=".global.NotebookApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <provider
            android:authorities="com.example.lenovo.notebook.provider"
            android:name="com.example.lenovo.notebook.BackDoor"></provider>
        <activity
            android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ReadActivity">
        </activity>
        <activity android:name=".WriteActivity">
        </activity>
        <activity android:name=".ReadArticleActivity">
        </activity>
        <activity android:name=".LoginActivity">
        </activity>
        <activity android:name=".RegistActivity">
        </activity>
        <activity android:name=".ResetActivity">
        </activity>
        <activity android:name=".IntroduceActivity">
        </activity>
    </application>
</manifest>
